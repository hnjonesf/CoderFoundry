
<script src="~/NgApps/CarFinderApp.js"></script>
<div class="container" ng-app="CarFinderApp">
    <div ng-view>
        <div ng-controller="DisplayController">
            <div class="col-sm-12">
                <h2 class="section-title">Pick a Car</h2>
            </div>
            <div class="col-sm-12">
                <div car-finder
                     year="selected.year"
                     make="selected.make"
                     model="selected.model"
                     trim="selected.trim"></div>
            </div>
        </div>
    </div>
</div>


